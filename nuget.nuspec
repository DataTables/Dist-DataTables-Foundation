<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>datatables.net-zf</id>
    <version>1.10.24</version>
    <description>Foundation integration for DataTables</description>
    <authors>SpryMedia Ltd</authors>
    <projectUrl>http://datatables.net</projectUrl>
    <license type="expression">MIT</license>
    <tags> DataTables jQuery table Foundation</tags>
    <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Foundation.git"/>
    <dependencies>
      <dependency id="jquery" version="1.7.0"/>
      <dependency id="datatables.net" version="1.10.23"/>
      <dependency id="foundation" version="6.0.0"/>
    </dependencies>
    <contentFiles>
      <files include="**/*" buildAction="Content"/>
    </contentFiles>
  </metadata>
  <files>
    <file src="js/dataTables.foundation.js" target="content\Scripts"/>
    <file src="js/dataTables.foundation.min.js" target="content\Scripts"/>
    <file src="css/dataTables.foundation.css" target="content\Content"/>
    <file src="css/dataTables.foundation.min.css" target="content\Content"/>
    <file src="js/dataTables.foundation.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="js/dataTables.foundation.min.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="css/dataTables.foundation.css" target="contentFiles\any\any\wwwroot\css"/>
    <file src="css/dataTables.foundation.min.css" target="contentFiles\any\any\wwwroot\css"/>
  </files>
</package>
